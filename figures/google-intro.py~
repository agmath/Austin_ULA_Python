from figures import *
from math import *

def node(p,r):
    newpath()
    arc(p,r,0,2*pi)
    closepath()

width=150
height=width
radius = 0.5

beginfigure("google-intro", width, height)
setupcoordinates([0,0,width,height], [-2,-2, 5,5])

points = [[0,3],[0,0],[3,3.5]]

for p in points:
    node(p, radius)
    fill(lightblue)
    stroke()



    

endfigure()


