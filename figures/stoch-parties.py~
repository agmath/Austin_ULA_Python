from figures import *

width=150
height = 120

beginfigure("stoch-parties", width, height)
setupcoordinates([0,0,width,height], [-2,-2,5,5])

P = [3,3]
Q = [4,0]
R = [0,0]

settexenv("ACTexConfig")

Label("P", P, alignment="cc").draw()
Label("Q", Q, alignment="cc").draw()
Label("R", R, alignment="cc").draw()
