from figures import *

width = 150
height = 100

beginfigure("eigen-3d-species", width, height)
setupcoordinates([0,0,width,height],[-1,-1,5,5])

settexenv("ACTexConfig")

Label("Juveniles", [2,3], alignment="cc").draw()
Label("Yearlings", [4,0], alignment="cc").draw()
Label("Adults", [0,0], alignment="cc").draw()

endfigure()
